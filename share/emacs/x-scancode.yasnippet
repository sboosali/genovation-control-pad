  KeyDescription

    { number      = $1
    , character   = $0
    , twinKeys    = def
    , isModifier  = def
    , isNumpad    = def
    , side        = def
    , codes1      = Codes { pressCode   = "_"
                          , releaseCode = "_"
                          }
    , codes2      = Codes { pressCode   =    "$2"
                          , releaseCode = "F0 $2"
                          }
    , codes3      = Codes { pressCode   =    "$2"
                          , releaseCode = "F0 $2"
                          }
    }
